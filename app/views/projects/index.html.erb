     
 
  <div class="row">
    <div class="large-12 columns">
      <h1>Projects <small>Choose. Then crowdfund.</small></h1>
      <hr/>
    </div>
  </div>
 
   
 
 
   
 
  <div class="row">
 
     
    <div class="large-9 columns" role="content">
       <%= render @projects %>
    </div>
 
     
 
 
     
 
    <aside class="large-3 columns">
      <div class="panel">
        <% if current_user %>
<h5>Hello, <%= current_user.name %></h5>
    <p>Here, we will display all of the projects that you are backing.</p>
        <%= link_to "Logout", session_path(current_user), method: :delete %>
<% else %>
<h5>Please <%= link_to "log in", new_session_path %> to create a project.</h5>
<% end %>

      </div>
 
    </aside>
 
     
  </div>
 
   